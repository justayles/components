{
  "name": "@justayles/components",
  "version": "0.2.1",
  "description": "A library of react components",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "prepare": "husky",
    "lint": "eslint src/components/**/*.*",
    "lint:fix": "eslint src/components/**/*.* --fix",
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "test": "jest --verbose --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "tw:update": "npx tailwindcss -i ./src/styles/index.css -o ./src/styles/main.css --watch"
  },
  "author": "Justin Ayles",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@chromatic-com/storybook": "^1.7.0",
    "@eslint/js": "^9.9.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-webpack5": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.4",
    "babel-jest": "^29.7.0",
    "clsx": "^2.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.21.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.2.5",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/justayles"
  },
  "lint-staged": {
    "src/components/**/*.*": [
      "pretty-quick --staged",
      "eslint src/components/**/*.* --fix"
    ]
